{
  "name": "exocloud-conecta-minha-saude-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "sls",
    "commit": "git-cz",
    "format": "prettier . --write --ignore-path ./tsconfig.json",
    "lint": "eslint --fix",
    "prepare": "husky install",
    "offline": "sls offline --httpPort 3333",
    "deploy": "rm -rf .build .serverless && sls deploy"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.1.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.8",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.9.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^13.2.0",
    "serverless-plugin-log-retention": "^2.0.0",
    "serverless-plugin-typescript": "^2.1.5",
    "serverless-prune-versions": "^1.0.4",
    "typescript": "*"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.438.0",
    "@aws-sdk/util-dynamodb": "^3.438.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "serverless": "^3.36.0",
    "serverless-http": "^3.2.0",
    "uuid": "^9.0.1",
    "yup": "^1.3.2"
  }
}
